  object QryCliente: TZQuery
    Connection = dtmPrincipal.ConexaoDB
    Active = True
    SQL.Strings = (
      'SELECT clienteId,'
      '       nome,'
      '       dataNascimento,'
      '       cpf,'
      '       email,'
      '       telefone,'
      '       endereco,'
      '       bairro,'
      '       cidade,'
      '       estado,'
      '       cep'#9'  '
      'FROM clientes'
      'ORDER BY nome')
    Params = <>
    Left = 312
    Top = 544
